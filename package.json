{
  "name": "consequent-postgres",
  "version": "2.0.0",
  "description": "Postgres actor and event storage adapters for consequent",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src/sql"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "lint": "eslint src spec --ext .ts",
    "lint:fix": "eslint src spec --ext .ts --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist dist-test",
    "prepublishOnly": "npm run clean && npm run build",
    "release": "npm run build && npm test && changeset publish"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/arobson/consequent-postgres.git"
  },
  "keywords": [
    "postgres",
    "consequent",
    "event-sourcing",
    "typescript",
    "esm"
  ],
  "author": "Alex Robson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/arobson/consequent-postgres/issues"
  },
  "homepage": "https://github.com/arobson/consequent-postgres#readme",
  "dependencies": {
    "pg": "^8.18.0",
    "pg-cursor": "^2.17.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.0",
    "@eslint/js": "^9.0.0",
    "@types/node": "^22.0.0",
    "@types/pg": "^8.11.0",
    "@vitest/coverage-v8": "^2.0.0",
    "eslint": "^9.0.0",
    "typescript": "^5.6.0",
    "typescript-eslint": "^8.0.0",
    "vitest": "^2.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
